<html>
<head>
    <style>
        @media screen {
            .noPrint {/*display:block !important;*/}
        }

            /* Print Only */
        @media print {
            .noPrint {display:none !important;}
        }

        .noPrint{ display: none;}

        #wrap {min-width: 960px;}
        .tableQualifications {border: 1px;}
        .tableQualifications table, th, td { border: 1px solid black; font-size: 11px; color: #000; border-spacing:0px;
            border-collapse:collapse;}
        .tableQualifications td { width: 45px; text-align: center; font-size: 11px;}
            /*.tableQualifications .azul {background-color: #2b34ee; color: #000; line-height: 0px;}*/
        .tableQualifications .azul {background-color: #2b34ee; color: #000; font-size: 12px; }
