<body>
<!-- PRO Framework Panel Begin -->
<div id="advanced">
    <span class="trigger"><strong></strong><em></em></span>
    <div class="bg_pro">
        <div class="pro_main">
            <a href="" class="pro_logo"></a>
            <div>
                <div style="float: left;">
                    <ul class="pro_menu">
                        <li><a href="index.html"><img src="{{ asset('expediente/images/pro_home.png') }}" alt=""></a></li>
                        
                    </ul>
                </div>
                <div style="float: right;">
                    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                    <ul class="pro_menu">
                        <li class="current"><a href="#">{{ app.user.username }}<span></span></a>
                            <ul>	
                                <li class="current"><a href="{{ path('_expediente_logout') }}">{% trans %}label.logout{% endtrans %}</a></li>
                            </ul>
                        </li>
                    </ul>
                    {% endif %}
                </div>    
            </div>
            
            
            <div class="clear"></div>
        </div>
    </div>
    <div class="bg_pro2"></div>
</div>
<!-- PRO Framework Panel End -->
<div class="bg-main">
	<header>
		<div class="container_24">
			<div class="wrapper">
				<div class="grid_17">
					<h1><a href="{{ path('TecnotekExpedienteBundle_homepage') }}">{% trans %}label.home{% endtrans %}</a></h1>
				</div>
				<div class="grid_7">
				</div>
			</div>
		</div>
		<nav class="main-menu">
                        
			<ul class="sf-menu">
                                {% if is_granted('ROLE_SUPERADMIN') %}
                                    <li class="current"><a href="{{ path('_expediente_sysadmin_administrador') }}"><em>{% trans %}label.usuarios{% endtrans %}</em><strong></strong></a>
                                        <ul>
						<li><a href="{{ path('_expediente_sysadmin_administrador') }}">{% trans %}label.usuarios.admin{% endtrans %}</a></li>
                                                <li><a href="{{ path('_expediente_sysadmin_administrador') }}">{% trans %}label.usuarios.coordinador{% endtrans %}</a></li>
                                                <li><a href="{{ path('_expediente_sysadmin_administrador') }}">{% trans %}label.usuarios.profesor{% endtrans %}</a></li>
					</ul>
                                    </li>
                                {% endif %}
				<li><a href="index-1.html"><em>services</em><strong></strong></a></li>
				<li><a href="index-6.html"><em>contacts</em><strong></strong></a></li>
			</ul>
			<div class="clear"></div>
		</nav>
	</header>
	<section class="padsection">
		{% block content %}{% endblock %}
	</section>	
</div>
<footer>
	<div class=" container_24">
		<div class="wrapper">
			<div class="grid_6 suffix_2">
				<h1 class="footer-logo"><a href="index.html">idealex</a></h1>
				<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit ultricies odio magna.</p>
				<p class="color1 privacy">&copy; 2012 <span>|</span>  <a href="index-7.html">Privacy Policy</a> <!--  --></p>
			</div>
			<div class="grid_7 suffix_2">
				<h4>Related Links</h4>
				<div class="wrapper">
					<div class="grid_4 alpha">
						<ul class="footer-list">
							<li><a href="more.html">About Us</a></li>
							<li><a href="more.html">Testimonials</a></li>
							<li><a href="more.html">Our Staff</a></li>
							<li><a href="more.html">Events &amp; Press</a></li>
							<li><a href="more.html">Contact Us</a></li>
						</ul>
					</div>
					<div class="grid_3 omega">
						<ul class="footer-list">
							<li><a href="more.html">Sign Up</a></li>
							<li><a href="more.html">Forums</a></li>
							<li><a href="more.html">Affiliate Program</a></li>
							<li><a href="more.html">FAQ</a></li>
						</ul>
					</div>
				</div>
			</div>
			<div class="grid_7">
				<h4>Follow Us</h4>
				<ul class="tooltips">
					<li><a href="more.html"><img src="{{ asset('expediente/images/icon1.png') }}" alt=""><span>RSS</span></a></li>
					<li><a href="more.html"><img src="{{ asset('expediente/images/icon3.png') }}" alt=""><span>Facebook</span></a></li>
					<li><a href="more.html"><img src="{{ asset('expediente/images/icon2.png') }}" alt=""><span>Twitter</span></a></li>
				</ul>
			</div>
		</div>
	</div>
</footer>
</body>
</html>