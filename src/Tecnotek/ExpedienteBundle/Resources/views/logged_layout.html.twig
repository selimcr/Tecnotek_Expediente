<!DOCTYPE html>
<html lang="en">
<head>
    <title>{% trans %}system.title{% endtrans %}</title>
    <meta charset="utf-8">
    <meta name="description" content={% trans %}system.description{% endtrans %}>
    <meta name="keywords" content="{% trans %}system.tags{% endtrans %}">
    <meta name="author" content="Selim Diaz A.">
    <link rel="stylesheet" href="{{ asset('expediente/css/style.css') }}" type="text/css" media="all" />
    <link rel="stylesheet" href="{{ asset('expediente/css/tabs.css') }}" type="text/css" media="all" />

    <link rel="stylesheet" href="http://code.jquery.com/ui/1.9.1/themes/base/jquery-ui.css" />
    <script src="http://code.jquery.com/jquery-1.7.1.min.js"></script>
    <script src="http://code.jquery.com/ui/1.9.1/jquery-ui.js"></script>
    <script type="text/javascript" src="{{ asset('expediente/js/spin.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('expediente/js/jquery.spin.js') }}"></script>
    {% block extracss %}
    {% endblock %}

    {% if is_granted('ROLE_SUPERADMIN') %}
    <script type="text/javascript" src="{{ asset('expediente/js/superadmin_application.js') }}"></script>
    {% else %}
        {% if is_granted('ROLE_PROFESOR') %}
            <script type="text/javascript" src="{{ asset('expediente/js/teacher_application.js') }}"></script>
        {% else %}

        {% endif %}
    {% endif %}

    {% block extrajs %}{% endblock %}
    <script type="text/javascript" src="{{ asset('expediente/js/script.js') }}"></script>
    
    
